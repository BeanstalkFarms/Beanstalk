{
  "name": "dex-ui",
  "description": "A user interface for Beanstalk Dex",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "vite --port 2424",
    "build": "yarn generate:graphql && tsc && vite build",
    "generate:graphql": "graphql-codegen --config codegen.ts",
    "generate": "echo \"DEX Generate\" && yarn generate:graphql",
    "lint": "eslint \"src/**/**/*.ts{,x}\"",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@beanstalk/sdk": "workspace:*",
    "@tanstack/react-query": "4.28.0",
    "@tanstack/react-query-devtools": "4.28.0",
    "connectkit": "1.2.1",
    "ethers": "^5",
    "graphql-request": "5.2.0",
    "loadash": "1.0.0",
    "numeral": "2.0.6",
    "prettier": "2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-jazzicon": "1.0.4",
    "react-router-dom": "^6.4.4",
    "styled-components": "5.3.9",
    "typescript": "4.8.4",
    "wagmi": "0.12.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.2.2",
    "@graphql-codegen/client-preset": "2.1.1",
    "@graphql-codegen/typescript-react-query": "4.1.0",
    "@types/numeral": "^2",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5",
    "@typescript-eslint/eslint-plugin": "4.13.0",
    "@typescript-eslint/parser": "4.13.0",
    "@vitejs/plugin-react": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "vite": "^3.2.5"
  }
}
