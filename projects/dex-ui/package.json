{
  "name": "dex-ui",
  "description": "A user interface for Beanstalk Dex",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite --port 2424",
    "build": "yarn all:build && yarn generate && tsc && vite build",
    "build:netlify": "./utils/build-netlify.sh",
    "generate:graphql": "graphql-codegen --config codegen.ts",
    "generate:typechain": "typechain --target=ethers-v5 --ts-nocheck --out-dir=./src/generated/types './src/abi/*.json'",
    "generate": "echo \"DEX Generate\" && yarn generate:graphql && yarn generate:typechain",
    "lint": "eslint \"src/**/**/*.ts{,x}\"",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@beanstalk/sdk": "workspace:*",
    "@tanstack/react-query": "5.28.4",
    "@tanstack/react-query-devtools": "5.28.4",
    "@typechain/ethers-v5": "10.2.1",
    "connectkit": "1.7.2",
    "ethers": "^5.7.2",
    "graphql-request": "5.2.0",
    "lightweight-charts": "4.1.3",
    "prettier": "3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "2.4.1",
    "react-jazzicon": "1.0.4",
    "react-router-dom": "^6.22.1",
    "styled-components": "5.3.11",
    "typechain": "8.1.1",
    "viem": "2.7.18",
    "wagmi": "2.5.7"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.2.2",
    "@graphql-codegen/client-preset": "2.1.1",
    "@graphql-codegen/typescript-react-query": "4.1.0",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "@typescript-eslint/parser": "7.1.0",
    "@vitejs/plugin-react": "4.2.1",
    "eslint": "8.57.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.34.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "typescript": "5.3.3",
    "vite": "5.1.4"
  }
}
