  "scripts": {
    "start": "vite --port 4173",
    "dev": "netlify dev --command \"yarn start\" --targetPort 4173",
    "serve": "vite preview",
    "build": "yarn generate && tsc && vite build",
    "build:dev": "tsc --noEmit --watch",
    "build:vite": "vite build",
    "build:storybook": "build-storybook -s public",
    "build:netlify": "./utils/build-netlify.sh",
    "build:fleek": "./utils/build-fleek.sh",
    "test": "yarn run vitest",
    "clean": "rm -rf ./src/generated && echo \"Cleaned src/generated.\"",
    "lint": "eslint --ext ts --ext tsx ./src",
    "generate": "echo '\nUI Generate' && yarn clean && yarn generate:typechain && yarn generate:graphql && echo \"Done generating files.\"",
    "generate:typechain": "echo \"Generating: typechain ethers-v5\" && typechain --target=ethers-v5 --ts-nocheck --out-dir=./src/generated '{./src/constants/abi/**/*.json,../../protocol/abi/Beanstalk.json}'",
    "generate:graphql": "echo \"Generating: graphql-codegen\" && graphql-codegen --config codegen.yml",
    "storybook": "start-storybook -p 6006 -s public",
    "postpack": "yarn generate"
  },
  "engines": {
    "node": ">=14"
  },
  "packageManager": "yarn@3.3.1",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
